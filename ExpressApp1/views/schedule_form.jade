form#scheduleForm(method="post",action="javascript:return false;")
	table
		tr
			td.form_title 案件番号
			td 
				input#entry_no(type="text" name="entry_no" readonly="true" placeholder="案件番号" size="10")
			td.form_title 試験明細番号
			td 
				input#quoto_detail_no(type="text" name="quoto_detail_no" placeholder="試験明細番号" size="10")
		tr
			td.form_title 試験項目名
			td 
				input#test_item(type="text" name="test_item" readonly="true" placeholder="試験項目名" size="10")
			td.form_title 試料名
			td 
				input#sample_name(type="text" name="sample_name" readonly="true" placeholder="試料名" size="10")
		tr
			td.form_title 開始日
			td 
				input.datepicker#start_date(type="text" name="start_date" placeholder="開始日" size="10")
			td.form_title 開始時間
			td 
				input#start_time(type="text" name="start_time" placeholder="開始時間" size="10")
		tr
			td.form_title 終了日 
			td 
				input.datepicker#end_date(type="text" name="end_date" placeholder="終了日" size="10")
			td.form_title 終了時間
			td 
				input#end_time(type="text" name="end_time" placeholder="終了時間" size="10")
		tr
			td.form_title AM/PM(安全性試験用） 
			td 
				select#am_pm(name="am_pm" placeholder="AM/PM")
					option(value="0") AM
					option(value="1") PM
			td.form_title パッチNo
			td 
				select#patch_no(name="patch_no" placeholder="パッチNo")
					option(value="1") 1
					option(value="2") 2
					option(value="3") 3
					option(value="4") 4
					option(value="5") 5
					option(value="6") 6
					option(value="7") 7
					option(value="8") 8
					option(value="9") 9
					option(value="10") 10
					option(value="11") 11
					option(value="12") 12
